<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="sku_seq" model="ir.sequence">
            <field name="name">sku_seq</field>
            <field name="code">sku_seq</field>
            <field name="prefix">%(year)s%(month)s%(day)s</field>
            <field name="padding">4</field>
            <field name="next_sku">180124AAAA</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- fba补货 -->
        <record id="fba_replenish_number" model="ir.sequence">
            <field name="name">fba_replenish_number</field>
            <field name="code">fba.replenish.number</field>
            <field name="prefix">FR%(year)s%(month)s%(day)s</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- 供应商结算单 -->
        <record id="supplier_settlement_number" model="ir.sequence">
            <field name="name">supplier_settlement_number</field>
            <field name="code">supplier.settlement.number</field>
            <field name="prefix">FR%(year)s%(month)s%(day)s</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- 申诉单 -->
        <record id="appeal_order_number" model="ir.sequence">
            <field name="name">appeal_order_number</field>
            <field name="code">appeal.order.number</field>
            <field name="prefix">AO%(year)s%(month)s%(day)s</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- 补货单 -->
        <record id="replenish_order_number" model="ir.sequence">
            <field name="name">replenish_order_number</field>
            <field name="code">replenish.order.number</field>
            <field name="prefix">RO%(year)s%(month)s%(day)s</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>