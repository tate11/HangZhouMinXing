<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
        <!-- 充值 -->
		<record id="account_charge_number" model="ir.sequence">
            <field name="name">account_charge_number</field>
            <field name="code">account.charge.number</field>
            <field name="prefix">AC%(year)s%(month)s%(day)s</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- 提现 -->
        <record id="account_cash_number" model="ir.sequence">
            <field name="name">account_cash_number</field>
            <field name="code">account.cash.number</field>
            <field name="prefix">Cash%(year)s%(month)s%(day)s</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- 发票 -->
        <record id="account_invoice_number" model="ir.sequence">
            <field name="name">account_invoice_number</field>
            <field name="code">account.invoice.number</field>
            <field name="prefix">AI%(year)s%(month)s%(day)s</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
	</data>
</odoo>